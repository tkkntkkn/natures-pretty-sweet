<Patch>
   <Operation Class="PatchOperationAddModExtension">
      <xpath>/Defs/TerrainDef[defName = "WaterDeep"]</xpath>
      <value>
         <li Class="TKKN_NPS.TerrainWeatherReactions">
            <floodTerrain />
            <wetTerrain />
            <freezeTerrain>TKKN_Ice</freezeTerrain>
            <dryTerrain />
            <baseOverride />
            <freezeAt>0</freezeAt>
			<wetAt></wetAt>
            <isSalty>False</isSalty>
			<holdFrost>False</holdFrost>
            <temperatureAdjust />
         </li>
      </value>
   </Operation>
