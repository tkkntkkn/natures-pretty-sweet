   <ThingDef ParentName="TreeBase">
      <defName>TKKN_PlantTreeRedwoodDead</defName>
      <label>redwood</label>
      <description>A huge, very slow growing tree. This one has toppled over due to its weight. \n\n Yeilds red wood, which is valuable and rare.</description>
      <graphicData>
         <texPath>TKKN_NPS/Things/Plant/TreeRedwoodDead</texPath>
         <graphicClass>Graphic_Random</graphicClass>
         <shadowData>
            <volume>(0.55, 0.2, 0.2)</volume>
            <offset>(0,0,-0.25)</offset>
         </shadowData>
      </graphicData>
      <statBases>
         <Beauty>15</Beauty>
      </statBases>
      <plant>
         <sowTags />
         <fertilityMin>0.7</fertilityMin>
         <fertilitySensitivity>0.7</fertilitySensitivity>
         <growDays>0</growDays>
         <harvestedThingDef>RedWoodLog</harvestedThingDef>
         <harvestWork>4000</harvestWork>
         <harvestYield>250</harvestYield>
			<wildClusterWeight>1</wildClusterWeight>
         <wildClusterRadius>3</wildClusterRadius>
         <visualSizeRange>
            <min>7</min>
            <max>9</max>
         </visualSizeRange>
         <topWindExposure>0</topWindExposure>
      </plant>
   </ThingDef>
   <ThingDef ParentName="PlantBase">
      <defName>TKKN_PlantFern</defName>
      <label>fern</label>
      <description>A soft, leafy plant that reproduces using spores.</description>
      <statBases>
         <MaxHitPoints>85</MaxHitPoints>
		 <Nutrition>0.25</Nutrition>
      </statBases>
      <pathCost>18</pathCost>
      <graphicData>
         <texPath>TKKN_NPS/Things/Plant/Ferns</texPath>
         <graphicClass>Graphic_Random</graphicClass>
      </graphicData>
      <hideAtSnowDepth>0.6</hideAtSnowDepth>
      <ingestible/>
      <plant>
         <fertilityMin>0.5</fertilityMin>
         <fertilitySensitivity>0.7</fertilitySensitivity>
         <growDays>3</growDays>
		 <dieIfLeafless>true</dieIfLeafless>
         <harvestWork>60</harvestWork>
         <maxMeshCount>9</maxMeshCount>
         <visualSizeRange>
            <min>0.7</min>
            <max>0.85</max>
         </visualSizeRange>
         <topWindExposure>0.4</topWindExposure>
      </plant>
   </ThingDef>
