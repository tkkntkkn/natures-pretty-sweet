<?xml version="1.0" encoding="UTF-8"?>
<Defs>
   <ThingDef ParentName="TreeBase">
      <defName>TKKN_PlantTreeRedwood</defName>
      <label>redwood</label>
      <description>A huge, very slow growing tree. Its roots are shallow, and when it gets too large, it topples over. \n\n Yields red wood, which is valuable and rare.</description>
      <graphicData>
         <texPath>TKKN_NPS/Things/Plant/TreeRedwoods</texPath>
         <graphicClass>Graphic_Random</graphicClass>
         <shadowData>
            <volume>(0.55, 0.2, 0.2)</volume>
            <offset>(0,0,-0.25)</offset>
         </shadowData>
      </graphicData>
      <altitudeLayer>BuildingOnTop</altitudeLayer>
      <statBases>
         <Beauty>15</Beauty>
      </statBases>
      <blockLight>true</blockLight>
      <plant>
         <sowTags />
         <fertilityMin>0.8</fertilityMin>
         <harvestedThingDef>RedWoodLog</harvestedThingDef>
         <fertilitySensitivity>0.8</fertilitySensitivity>
         <growDays>300</growDays>
         <harvestWork>3000</harvestWork>
         <harvestYield>225</harvestYield>
			<wildClusterWeight>8</wildClusterWeight>
         <wildClusterRadius>10</wildClusterRadius>
         <visualSizeRange>
            <min>8</min>
            <max>12</max>
         </visualSizeRange>
      </plant>
   </ThingDef>
   <ThingDef ParentName="TreeBase">
      <defName>TKKN_OldGrowth_PlantTreeOak</defName>
      <label>old growth oak tree</label>
      <description>A strong hardwood tree. Slow-going, but its wood is very strong.</description>
      <graphicData>
         <texPath>Things/Plant/TreeOak</texPath>
         <graphicClass>Graphic_Random</graphicClass>
      </graphicData>
      <statBases>
         <Beauty>2</Beauty>
      </statBases>
      <plant>
         <sowTags />
         <fertilityMin>0.4</fertilityMin>
         <growDays>60</growDays>
         <leaflessGraphicPath>Things/Plant/TreeOak_Leafless</leaflessGraphicPath>
         <harvestWork>1000</harvestWork>
         <harvestYield>75</harvestYield>
			<wildClusterWeight>32</wildClusterWeight>
         <wildClusterRadius>5</wildClusterRadius>
         <visualSizeRange>
            <min>2</min>
            <max>4</max>
         </visualSizeRange>
      </plant>
   </ThingDef>
