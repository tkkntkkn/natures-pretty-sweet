  <ThingDef ParentName="BuildingNaturalBase">
    <defName>TKKN_SteamVent</defName>
    <thingClass>TKKN_NPS.Building_SteamVent</thingClass>
    <label>steam vent</label>
    <graphicData>
      <texPath>TKKN_NPS/Things/Building/Steamvent</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>Transparent</shaderType>
      <drawSize>(1,1)</drawSize>
    </graphicData>
    <altitudeLayer>Floor</altitudeLayer>
		<rotatable>true</rotatable>
    <destroyable>true</destroyable>
    <useHitPoints>false</useHitPoints>
    <tickerType>Normal</tickerType>
    <selectable>true</selectable>
    <description>Temporary natural steam source from lava rock.</description>
    <size>(1,1)</size>
    <neverOverlapFloors>true</neverOverlapFloors>
    <blockPlants>true</blockPlants>
    <building>
      <isEdifice>false</isEdifice>
      <claimable>false</claimable>
      <deconstructible>false</deconstructible>
    </building>
  </ThingDef>

  
  
  
  <ThingDef ParentName="BuildingBase">
    <defName>TKKN_SteamGenerator</defName>
    <label>steam generator</label>
    <thingClass>Building</thingClass>
    <graphicData>
      <texPath>TKKN_NPS/Things/Building/SteamGenerator</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3,3)</drawSize>
      <damageData>
        <rect>(0.3,0,5.4,6)</rect>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
	<minifiedDef>MinifiedFurniture</minifiedDef>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
		<scatterableOnMapGen>false</scatterableOnMapGen>
		<fillPercent>1</fillPercent>
    <blockLight>true</blockLight>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>500</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>-30</Beauty>
    </statBases>
    <tickerType>Normal</tickerType>
    <description>Produces electricity from steam vents in lava fields. Must be placed on a steam vent.</description>
    <size>(3,3)</size>
    <costList>
      <Steel>100</Steel>
      <Component>1</Component>
    </costList>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerPlantSteam</compClass>
        <basePowerConsumption>-400</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
      </li>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <rotatable>false</rotatable>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <designationCategory>Power</designationCategory>
    <staticSunShadowHeight>.5</staticSunShadowHeight>
    <placeWorkers>
      <li>TKKN_NPS.PlaceWorker_OnSteamVent</li>
    </placeWorkers>
    <building>
			<ai_chillDestination>false</ai_chillDestination>
			<soundAmbient>GeothermalPlant_Ambience</soundAmbient>
    </building>
    <constructEffect>ConstructMetal</constructEffect>
    <designationHotKey>Misc6</designationHotKey>
    <researchPrerequisites>
      <li>Electricity</li>	
	</researchPrerequisites>
    <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
  </ThingDef>
  

  </Defs>