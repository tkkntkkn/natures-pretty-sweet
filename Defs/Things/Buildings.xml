<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>TKKN_Sculpture_Shell</defName>
    <label>sea shell</label>
    <description>The beautiful, hard, protective shell of long dead sea animals.</description>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <texPath>TKKN_NPS/Things/Sculptures/Shells</texPath>
      <drawRotated>false</drawRotated>
    </graphicData>
    <fillPercent>0.15</fillPercent>
    <altitudeLayer>Building</altitudeLayer>
    <hasInteractionCell>false</hasInteractionCell>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <minifiedDef>MinifiedFurniture</minifiedDef>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <MarketValue>200</MarketValue>
      <Mass>7.5</Mass>
      <EntertainmentStrengthFactor>1</EntertainmentStrengthFactor>
    </statBases>
		<comps>
			<li Class="CompProperties_RoomIdentifier">
				<roomStat>Impressiveness</roomStat>
			</li>
		</comps>
  </ThingDef>
  
  
  <ThingDef ParentName="BuildingBase">
    <defName>TKKN_RedwoodDead</defName>
    <label>redwood</label>
    <description>A huge, very slow growing tree. Its roots are shallow, and when it gets too large, it topples over. \n\n Yeilds red wood, which is valuable and rare.</description>
      <graphicData>
         <texPath>TKKN_NPS/Things/Plant/RedwoodDead</texPath>
         <graphicClass>Graphic_Random</graphicClass>
         <drawRotated>false</drawRotated>
         <shadowData>
            <volume>(0.55, 0.2, 0.2)</volume>
            <offset>(0,0,-0.25)</offset>
         </shadowData>
      <drawSize>(9,3)</drawSize>
    </graphicData>
    <comps />
    <tickerType>Rare</tickerType>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <selectable>true</selectable>
    <destroyable>true</destroyable>
    <useHitPoints>true</useHitPoints>
    <repairEffect/>
    <costList>
      <RedWoodLog>200</RedWoodLog>
   </costList>    
   <statBases>
      <MaxHitPoints>3000</MaxHitPoints>
      <WorkToBuild>0</WorkToBuild>
      <Flammability>0</Flammability>
      <Beauty>15</Beauty>
    </statBases>
    <fillPercent>0.15</fillPercent>
		<building>
			<ai_chillDestination>true</ai_chillDestination>
		</building>
    <rotatable>false</rotatable>
  </ThingDef>
  
  
  
  <ThingDef ParentName="BuildingBase">
    <defName>TKKN_Lava_Spring</defName>
    <label>Lava Eruption</label>
    <thingClass>TKKN_NPS.Lava_Spring</thingClass>	
    <description>Spews molten lava, which hardens into lava rock.</description>
    <graphicData>
      <texPath>TKKN_NPS/Things/Building/coldSpring</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <damageData>
        <rect>(0.2,0.2,0.6,0.6)</rect>
      </damageData>
    </graphicData>
    <comps>
	   <li Class="CompProperties_Glower">
        <glowRadius>12</glowRadius>
        <glowColor>(128,148,0,60)</glowColor>
      </li>
      <li Class="TKKN_NPS.CompProperties_SpringLava">
        <radius>7</radius>
        <dryTile>TKKN_LavaRock_RoughHewn</dryTile>
        <wetTile>TKKN_Lava</wetTile>      
        <deepTile>TKKN_Lava</deepTile>
        <temperature>+20</temperature>
        <spawnProp>TKKN_LavaRock</spawnProp>
		<weight>2000</weight>
		<howOftenToChange>8000</howOftenToChange>
		<commonBiome></commonBiome>
		<canReproduce>true</canReproduce>
		<borderSize>2</borderSize>
		<startingRadius>0</startingRadius>
      </li>
    </comps>
    <tickerType>Rare</tickerType>
    <altitudeLayer>Floor</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <selectable>false</selectable>
    <destroyable>true</destroyable>
    <useHitPoints>false</useHitPoints>
    <statBases>
      <Flammability>0</Flammability>
      <Beauty>25</Beauty>
    </statBases>
    <fillPercent>0.15</fillPercent>
		<building>
			<ai_chillDestination>true</ai_chillDestination>
		</building>
    <rotatable>false</rotatable>
  </ThingDef>
  

